<!DOCTYPE settings SYSTEM "../settings.dtd">

<!-- A conf to use to tune a new A/C -->

<settings>
  <dl_settings>
    <dl_settings NAME="performance">

      <dl_settings NAME="estimates">
        <dl_setting MAX="2" MIN="-1" STEP="0.01" VAR="ac_char_climb_pitch" shortname="climb_pitch"  module="guidance/energy_ctrl" unit="rad" alt_unit="deg"/>
        <dl_setting MAX="20" MIN="0" STEP="0.01" VAR="ac_char_climb_max" shortname="climb_max" />
        <dl_setting MAX="1" MIN="-2" STEP="0.01" VAR="ac_char_descend_pitch" shortname="descend_pitch"  unit="rad" alt_unit="deg" />
        <dl_setting MAX="0" MIN="-15" STEP="0.01" VAR="ac_char_descend_max" shortname="descend_max" />
        <dl_setting MAX="1" MIN="0" STEP="0.01" VAR="ac_char_cruise_throttle" shortname="cruise_thr" />
        <dl_setting MAX="1" MIN="-1" STEP="0.01" VAR="ac_char_cruise_pitch" shortname="cruise_pitch"  unit="rad" alt_unit="deg"/>
        <dl_setting MAX="1" MIN="0" STEP="1" VAR="ac_char_train" shortname="learn"  />
      </dl_settings>

    </dl_settings>
  </dl_settings>
</settings>
